{
  "name": "mgmg",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "run": "npm run api && ng s --o",
    "serve": "ng serve --o",
    "serve:bescript": "ng serve --configuration backend --o",
    "serve:be": "npm i -f && npm run serve:bescript",
    "build": "ng build",
    "stage": "npm i -f &&  node --max_old_space_size=8000 node_modules/@angular/cli/bin/ng build --configuration stage",
    "build-stage": "npm i -f &&  node --max_old_space_size=8000 node_modules/@angular/cli/bin/ng build --configuration stage",
    "release": "npm i -f && ng build --configuration release",
    "build-prod": "npm i -f && ng build --configuration production",
    "build-dep": "npm i -f && ng build --configuration deployment",
    "start": "node --max_old_space_size=8000 node_modules/@angular/cli/bin/ng serve --o",
    "start:windows": "ng serve --port 44441 --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key",
    "start:default": "ng serve --port 44441 --ssl --ssl-cert $HOME/.aspnet/https/${npm_package_name}.pem --ssl-key $HOME/.aspnet/https/${npm_package_name}.key",
    "build:ssr": "ng run mgmg:server:dev",
    "build:w": "ng build --watch",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postbuild": "node src/environments/postbuild.js",
    "myCustomMemoryComm": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "api-common": "ng-openapi-gen --config openapi/openapi-conf-common.json",
    "api-elites": "ng-openapi-gen --config openapi/openapi-conf-elites.json",
    "api": "npm run api-pm && npm run api-common && npm run api-tm",
    "build-full": "npm i -f && node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build && cd ../BE && git pull origin master && dotnet publish -c Release -r win-x64 --output ../build "
  },
